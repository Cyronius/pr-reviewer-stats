{
  "name": "pr-velocity-dashboard",
  "version": "1.0.0",
  "description": "PR velocity tracking for Azure DevOps",
  "private": true,
  "dependencies": {
    "chart.js": "^4.4.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.2.0",
    "dotenv": "^16.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^9.2.1",
    "http-proxy-middleware": "^3.0.5",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"react-scripts start\"",
    "start:client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "fetch": "tsx fetch-pr-velocity.ts --org itkennel --output pr-velocity.csv",
    "server": "tsx server.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
